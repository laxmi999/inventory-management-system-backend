{
  "name": "inventory-management",
  "version": "1.0.0",
  "description": "inventory management for small businesses",
  "main": "dist/app.js",
  "scripts": {
    "codegen": "kysely-codegen --dialect postgres --out-file ./src/db/kysely-types.d.ts",
    "prebuild": "eslint src --ext .ts --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon --watch src --ext ts --exec tsx src/app.ts",
    "migrator": "tsx src/db/migrator.ts",
    "migrate:down": "tsx  src/db/undoMigration.ts",
    "test": "tsx src/middlewares/auth.ts"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "swagger-ui": "^5.21.0",
    "swagger-ui-dist": "^5.21.0",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/express": "^4.17.21",
    "@types/hast": "^3.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.64",
    "@types/pg": "^8.11.11",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/unist": "^3.0.3",
    "@types/use-sync-external-store": "^1.5.0",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.30.1"
  }
}
