{
  "name": "inventory-management",
  "version": "1.0.0",
  "description": "inventory management for small businesses",
  "main": "dist/app.js",
  "scripts": {
    "codegen": "kysely-codegen --dialect postgres --camel-case --out-file ./src/db/kysely-types.d.ts",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon --watch src --ext ts --exec tsx src/app.ts",
    "migrator": "tsx src/db/migrator.ts",
    "test": "tsx src/db/pathTest.ts"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.64",
    "@types/pg": "^8.11.11",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  }
}
